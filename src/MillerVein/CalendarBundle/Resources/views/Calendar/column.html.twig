<div class="calendar-column">
    <div class="column-header">{{column.label}}</div>
    <div class="column-hours">
        {% if column.hours.name is defined %}
        {{ column.hours.name }}
        {% endif %}
    </div>
    <div class="column-body">
        {% for timeslot in column.timeslots %}
            {% include 'MillerVeinCalendarBundle:Calendar:timeslot.html.twig' %}
        {% endfor %}
    </div>
</div>