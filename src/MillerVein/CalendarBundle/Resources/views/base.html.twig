{% extends "::base.html.twig" %}
{#Title#}
{% block title %}{% endblock %}

{#Stylesheets#}
{% block stylesheets %}
    {{ parent() }}
    {% stylesheets 'bundles/millerveincalendar/css/jqueryui/*'  filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}

{#Javascripts#}
{% block javascripts %}
    {{ parent()}}
    {% javascripts 
    '@MillerVeinCalendarBundle/Resources/public/js/jquery/*'
    '@MillerVeinCalendarBundle/Resources/public/js/jqueryui/*' %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}

{#Body#}
{% block body %}

{% endblock %}